{
 "name": "@n8n/chat",
 "version": "0.55.0",
 "scripts": {
   "dev": "npm run storybook",
   "build": "npm run build:bundle",
   "build:vite": "cross-env vite build",
   "build:bundle": "cross-env INCLUDE_VUE=true vite build --config vite.config.simple.js",
   "build:coolify": "npm run build:bundle && if not exist public mkdir public && xcopy dist\\* public\\ /E /Y && copy demo-pages.html public\\index.html",
   "preview": "vite preview",
   "test:dev": "vitest",
   "test": "vitest run",
   "typecheck": "vue-tsc --noEmit",
   "lint": "eslint src --quiet",
   "lint:fix": "eslint src --fix",
   "lint:styles": "stylelint \"src/**/*.{scss,sass,vue}\" --cache",
   "lint:styles:fix": "stylelint \"src/**/*.{scss,sass,vue}\" --fix --cache",
   "format": "biome format --write src .storybook && prettier --write src/ --ignore-path ../../../../.prettierignore",
   "format:check": "biome ci src .storybook && prettier --check src/ --ignore-path ../../../../.prettierignore",
   "storybook": "storybook dev -p 6006 --no-open",
   "build:storybook": "storybook build"
 },
 "types": "./dist/index.d.ts",
 "main": "./dist/chat.umd.js",
 "module": "./dist/chat.es.js",
 "exports": {
   ".": {
     "types": "./dist/index.d.ts",
     "import": "./dist/chat.es.js",
     "require": "./dist/chat.umd.js"
   },
   "./style.css": {
     "import": "./dist/style.css",
     "require": "./dist/style.css"
   },
   "./*": {
     "import": "./*",
     "require": "./*"
   }
 },
"dependencies": {
  "@vueuse/core": "^10.9.0",
  "highlight.js": "^11.9.0",
  "markdown-it-link-attributes": "^4.0.1",
  "uuid": "^9.0.1",
  "vue": "^3.4.0",
  "vue-markdown-render": "^2.1.0"
},
"devDependencies": {
  "@iconify-json/mdi": "^1.1.54",
  "@vitejs/plugin-vue": "^4.5.0",
  "unplugin-icons": "^0.19.0",
  "vite": "^5.0.0",
  "vite-plugin-dts": "^4.5.3",
  "vue-tsc": "^1.8.27",
  "cross-env": "^7.0.3"
},
 "files": [
   "README.md",
   "dist"
 ]
}